<div class="container">
    <div class="row">
      <div class="col-1"></div>
      <div class="col-10">
      <h5><span>新規会員登録</span></h5>
      </div>
    </div>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "public/shared/error_messages", resource: resource %>
<div class="login_f"></div>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-1">
      名前
      </div>
      <div class="col-1"></div>
      <div class="col-1">
      <%= f.label :last_name,"(姓)" %>
      </div>
      <div class="col-1">
      <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
      </div>
      <div class="col-1"></div>
      <div class="col-1">
      <%= f.label :first_name,"(名)" %>
      </div>
      <div class="col-1">
      <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
      </div>
    </div>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-1">
      フリガナ
      </div>
      <div class="col-1"></div>
      <div class="col-1">
      <%= f.label :last_name_kana,"(セイ)" %>
      </div>
      <div class="col-1">
      <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "last_name_kana" %>
      </div>
      <div class="col-1"></div>
      <div class="col-1">
      <%= f.label :first_name_kana,"(メイ)" %>
      </div>
      <div class="col-1">
      <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "first_name_kana" %>
      </div>
    </div>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-2">
      <%= f.label :email,"メールアドレス" %>
      </div>
      <div class="col-1"></div>
      <div class="col-1">
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
      </div>
    </div>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-3">
      <%= f.label :postal_code,"郵便番号（ハイフンなし）" %>
      </div>
      <!--<div class="col-1"></div>-->
      <div class="col-2">
      <%= f.text_field :postal_code, autofocus: true, autocomplete: "postal_code" %>
      </div>
    </div>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-2">
      <%= f.label :address,"住所" %>
      </div>
      <div class="col-1"></div>
      <div class="col-2">
      <%= f.text_field :address, autofocus: true, autocomplete: "addresses", size: 60 %>
      </div>
    </div>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-3">
      <%= f.label :telephone_number,"電話番号（ハイフンなし）" %>
      </div>
      <!--<div class="col-1"></div>-->
      <div class="col-2">
      <%= f.text_field :telephone_namber, autofocus: true, autocomplete: "telephone_namber" %>
      </div>
    </div>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-3">
      <%= f.label :password, "パスワード(６文字以上)" %>
      </div>
      <!--<div class="col-1"></div>-->
      <div class="col-2">
      <%= f.password_field :password, autocomplete: "new-password" %>
      </div>
    </div>
  <div class="row">
      <div class="col-1"></div>
      <div class="col-2">
      <%= f.label :password_confirmation,"パスワード(確認用)" %>
      </div>
      <div class="col-1"></div>
      <div class="col-2">
      <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
      </div>
  </div>
  <div class="row">
      <div class="col-5"></div>
      <div class="col-2">
      <div class="actions-1">
      <%= f.submit "新規登録", class: "btn btn-success" %>
      </div>
      </div>
  </div>
<% end %>
    <div class="row">
      <div class="col-1"></div>
      <div class="col-5">
      <h5><span>既に登録済みの方</span></h5>
      </div>
    </div>
    <div class="row">
      <div class="col-2"></div>
      <div class="col-5">
          <%= link_to "こちら", new_customer_session_path %>からログインしてください。
      </div>
    </div>
</div>