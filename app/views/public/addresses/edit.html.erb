<h4>配送先編集</h4>
  <%= form_with model: @address,url: public_address_path(@address.id), method: :patch do |f| %>
  <label>郵便番号（ハイフンなし）</label>
  <%= f.text_field :postal_code, autofocus: true, id:"inputTitle"%><br>
  <label>住所</label>
  <%= f.text_field :addresses, autofocus: true, id:"inputTitle"%><br>
  <label>宛名</label>
  <%= f.text_field :name, autofocus: true, id:"inputTitle"%><br>

  <%= f.submit "変更内容を保存", class: "btn btn-success" %>
  <% end %>