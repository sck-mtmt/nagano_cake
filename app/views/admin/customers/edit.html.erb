<div class="container">
      <div class="row">
        <div class="col-4">
        <h5><span><%= @customer.last_name %><%= @customer.first_name %>さんの会員情報編集</span></h5>
        </div>
      </div>
<%= form_with model: @customer,url: admin_customer_path,method: :patch do |f| %>
      <div class="row">
        <div class="col-1">
        <h7>会員ID</h7>
        </div>
        <div class="col-1"></div>
        <div class="col-1">
        <%= @customer.id %>
        </div>
      </div>
      <div class="row">
        <div class="col-1">
       <h7>氏名</h7>
       </div>
       <div class="col-1"></div>
        <div class="col-5">
        <%= f.text_field :last_name %>
        <%= f.text_field :first_name %>
        </div>
      </div>
      <div class="row">
        <div class="col-1">
        <h7>カナ</h7>
        </div>
       <div class="col-1"></div>
        <div class="col-5">
        <%= f.text_field :last_name_kana %>
        <%= f.text_field :first_name_kana %>
        </div>
      </div>
      <div class="row">
        <div class="col-1">
        <h7>郵便番号</h7>
        </div>
       <div class="col-1"></div>
        <div class="col-8">
        <%= f.text_field :postal_code %>
        </div>
      </div>
      <div class="row">
        <div class="col-1">
        <h7>住所</h7>
        </div>
       <div class="col-1"></div>
        <div class="col-8">
        <%= f.text_field :address %>
        </div>
      </div>
      <div class="row">
        <div class="col-1">
      <h7>電話番号</h7>
     </div>
       <div class="col-1"></div>
        <div class="col-5">
        <%= f.text_field :telephone_namber %>
        </div>
      </div>
      <div class="row">
        <div class="col-2">
        <h7>メールアドレス</h7>
        </div>
       <!--<div class="col-sm-1"></div>-->
        <div class="col-8">
        <%= f.text_field :email %>
        </div>
      </div>
      <div class="row">
        <div class="col-2">
        <h7>会員ステータス</h7>
        </div>
       <!--<div class="col-1"></div>-->
        <div class="col-8">
        <%= f.radio_button :is_deleted,false %>有効
        <%= f.radio_button :is_deleted,true %>退会
        </div>
      </div>
      <div class="row">
        <div class="col-2"></div>
        <div class="col-4">
        <div class="actions">
        <%= f.submit'編集内容を保存', class: "btn btn-success" %>
        </div>
        </div>
      </div>
<% end %>
</div>